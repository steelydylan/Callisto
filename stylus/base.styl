@font-face
  font-family: 'Open Sans'
  font-style: normal
  font-weight: 400
  src: local('Open Sans'), local('OpenSans'), url(http://themes.googleusercontent.com/static/fonts/opensans/v9/cJZKeOuBrn4kERxqtaUH3bO3LdcAZYWl9Si6vvxL-qU.woff) format('woff')

body
	font-family: 'Open Sans', sans-serif;
	
*
  box-sizing: border-box

.clearfix:after,
.clearfix:before
	content: '';
	display: table;

.clearfix:after
	clear: both;

.clearfix 
	zoom: 1;

.{prefix}float-left
	float:left !important

.{prefix}float-right
	float:right !important

.{prefix}h1
	font-size:h1Size

.{prefix}h2
	font-size:h2Size

.{prefix}h3
	font-size:h3Size

.{prefix}h4
	font-size:h4Size

.{prefix}h5
	font-size:h5Size

.{prefix}container
	max-width 960px
	margin 0 auto 0 auto
